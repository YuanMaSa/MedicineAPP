<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FAFAFA"
    tools:context="com.example.carrie.carrie_test1.historyrecord">

    <TableLayout
        android:layout_width="350dp"
        android:layout_height="495dp"
        tools:layout_editor_absoluteX="4dp"
        tools:layout_editor_absoluteY="2dp">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <LinearLayout
                android:layout_width="350sp"
                android:layout_height="100sp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5sp"
                android:layout_marginTop="5sp"
                android:layout_weight="1"

                android:background="@drawable/historyborder"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView32"
                    android:layout_width="270sp"
                    android:layout_height="50sp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="0sp"
                    android:layout_marginTop="20sp"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:text="頭痛"
                    android:textColor="#444444"
                    android:textSize="15pt"
                    tools:layout_editor_absoluteX="5sp"
                    tools:layout_editor_absoluteY="5sp" />

                <TextView
                    android:id="@+id/textView34"
                    android:layout_width="330sp"
                    android:layout_height="50sp"
                    android:layout_marginLeft="10sp"
                    android:layout_marginRight="0sp"
                    android:layout_marginTop="15sp"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:text="6/1 ~ 6/5"
                    android:textColor="#444444"
                    android:textSize="12pt"
                    />

                <TextView
                    android:id="@+id/textView33"
                    android:layout_width="300sp"
                    android:layout_height="50sp"
                    android:layout_marginLeft="0sp"
                    android:layout_marginRight="0sp"
                    android:layout_marginTop="15sp"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:text="60%"
                    android:textColor="#444444"
                    android:textSize="12pt" />

            </LinearLayout>

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <LinearLayout
                android:layout_width="350sp"
                android:layout_height="100sp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5sp"
                android:layout_marginTop="5sp"
                android:layout_weight="1"

                android:background="@drawable/historyborder"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView32"
                    android:layout_width="250sp"
                    android:layout_height="50sp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="0sp"
                    android:layout_marginTop="20sp"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:text="發燒"
                    android:textColor="#444444"
                    android:textSize="15pt"
                    tools:layout_editor_absoluteX="5sp"
                    tools:layout_editor_absoluteY="5sp" />

                <TextView
                    android:id="@+id/textView34"
                    android:layout_width="200sp"
                    android:layout_height="50sp"
                    android:layout_marginLeft="-130sp"
                    android:layout_marginRight="0sp"
                    android:layout_marginTop="15sp"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:text="7/1 ~ 7/5"
                    android:textColor="#444444"
                    android:textSize="12pt" />

                <TextView
                    android:id="@+id/textView33"
                    android:layout_width="150sp"
                    android:layout_height="50sp"
                    android:layout_marginLeft="-40sp"
                    android:layout_marginRight="0sp"
                    android:layout_marginTop="15sp"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:text="100%"
                    android:textColor="#444444"
                    android:textSize="12pt" />

                <ImageButton
                    android:id="@+id/imageButton11"
                    android:layout_width="100dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="-35sp"
                    android:layout_marginRight="0sp"
                    android:layout_marginTop="20sp"
                    android:layout_weight="1"
                    android:background="@drawable/send"
                    android:onClick="gotocomment" />

            </LinearLayout>

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <LinearLayout
                android:layout_width="350sp"
                android:layout_height="100sp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5sp"
                android:layout_marginTop="5sp"
                android:layout_weight="1"

                android:background="@drawable/historyborder"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView32"
                    android:layout_width="270sp"
                    android:layout_height="50sp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="0sp"
                    android:layout_marginTop="20sp"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:text="咳嗽"
                    android:textColor="#444444"
                    android:textSize="15pt"
                    tools:layout_editor_absoluteX="5sp"
                    tools:layout_editor_absoluteY="5sp" />

                <TextView
                    android:id="@+id/textView34"
                    android:layout_width="330sp"
                    android:layout_height="50sp"
                    android:layout_marginLeft="10sp"
                    android:layout_marginRight="0sp"
                    android:layout_marginTop="15sp"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:text="8/1 ~ 8/5"
                    android:textColor="#444444"
                    android:textSize="12pt"
                    />

                <TextView
                    android:id="@+id/textView33"
                    android:layout_width="300sp"
                    android:layout_height="50sp"
                    android:layout_marginLeft="0sp"
                    android:layout_marginRight="0sp"
                    android:layout_marginTop="15sp"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:text="60%"
                    android:textColor="#444444"
                    android:textSize="12pt" />

            </LinearLayout>

        </TableRow>
    </TableLayout>

</android.support.constraint.ConstraintLayout>
